
cd /; ls -laR | grep ^- | awk -F ' ' '$2 > 1' | grep ^- > /tmp/ha.txt;clear; cat /tmp/ha.txt | grep ^- | awk -F " " '{print $NF}' > /tmp/heh.txt; for i in ` grep -Fxvf safehlinks.txt /tmp/heh.txt`; do find | grep -w $i > /tmp/listofhardlinks; done; cat /tmp/listofhardlinks

